  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
  font-weight: normal;
}
._a {
  text-decoration: none;
  color: #000;
}
._a,
label,
button,
input,
._select {
  -webkit-tap-highlight-color: rgba(0, 0, 0, 0);
}
._img {
  border: 0;
}
page {
  background: #fff;
  color: #666;
}
page,
page,
._div,
._dl,
._dt,
._dd,
._ol,
._ul,
._li,
._h1,
._h2,
._h3,
._h4,
